{
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "jsx": "react",
    "lib": [
      "es6"
    ],
    "moduleResolution": "node",
    "noEmit": true,
    "strict": false,
    "target": "esnext",
    "skipLibCheck": true /* Skip type checking all .d.ts files. */,
  },
  "types": [
    "node"
  ],
  "exclude": [
    "node_modules"
  ]
}