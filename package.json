{
  "name": "rn_boilerplate",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android --active-arch-only --variant=devDebug --appIdSuffix=dev",
    "android:staging": "react-native run-android --active-arch-only --variant=stagingDebug --appIdSuffix=staging",
    "android:prod": "react-native run-android --active-arch-only --variant=prodDebug --appIdSuffix=prod",
    "ios": "react-native run-ios --scheme \"dev\"",
    "ios:staging": "react-native run-ios --scheme \"staging\"",
    "ios:prod": "react-native run-ios --scheme \"prod\"",
    "lint": "eslint index.js app test --ext js, jsx, ts,tsx",
    "lint:fix": "eslint app --fix --ext js, jsx, ts,tsx app",
    "format": "prettier --write \"app/**/*.{js,jsx,ts,tsx}\"",
    "start": "react-native start --reset-cache",
    "icon": "npx rn-ml appicon -s",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --detectOpenHandles --runInBand --coverage --passWithNoTests",
    "test:report": "jest --detectOpenHandles --runInBand --reporters=default --reporters=jest-junit --coverage --passWithNoTests",
    "bundle:ios": "react-native bundle --entry-file index.js --platform ios --dev false --bundle-output ios/main.jsbundle --assets-dest ios",
    "bundle:android": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "adb": "adb reverse tcp:9090 tcp:9090 && adb reverse tcp:3000 tcp:3000 && adb reverse tcp:9001 tcp:9001 && adb reverse tcp:8081 tcp:8081",
    "release:android": "cd android && rm -rf app/src/main/res/drawable-* && ./gradlew assembleRelease && cd - && echo 'APK generated in ./android/app/build/outputs/apk/release/app-release.apk'",
    "release:ios": "cd ios && fastlane ios build",
    "patch": "patch-package",
    "splash": "npx react-native generate-bootsplash ./assets/splash/original_icon.png --background-color=FFFFFF   --logo-width=100 --assets-path=./assets --flavor=main",
    "pod": "cd ios/ && pod install && cd ..",
    "clean": "npx react-native-clean-project",
    "clean-all": "npx react-native clean-project-auto",
    "postinstall": "node ./bin/postInstall"
  },
  "engines": {
    "node": ">= 16.0.0"
  },
  "dependencies": {
    "@ant-design/react-native": "^5.0.4",
    "@hookform/resolvers": "^3.1.0",
    "@react-native-async-storage/async-storage": "^1.18.1",
    "@react-navigation/bottom-tabs": "^6.5.7",
    "@react-navigation/drawer": "^6.6.2",
    "@react-navigation/native": "^6.1.6",
    "@react-navigation/native-stack": "^6.9.12",
    "@react-navigation/stack": "^6.3.16",
    "@reduxjs/toolkit": "^1.9.5",
    "@sentry/react-native": "^5.3.1",
    "axios": "^1.3.5",
    "i18next": "^22.5.0",
    "i18next-http-backend": "^2.2.1",
    "lodash": "^4.17.21",
    "patch-package": "^7.0.0",
    "postinstall-prepare": "^2.0.0",
    "react": "^18.2.0",
    "react-hook-form": "^7.43.9",
    "react-i18next": "^12.3.1",
    "react-native": "0.71.6",
    "react-native-bootsplash": "^4.6.0",
    "react-native-config": "^1.5.1",
    "react-native-device-info": "^10.6.0",
    "react-native-gesture-handler": "^1.0.12",
    "react-native-linear-gradient": "^2.6.2",
    "react-native-modalize": "^2.1.1",
    "react-native-reanimated": "^3.1.0",
    "react-native-safe-area-context": "^4.5.1",
    "react-native-screens": "^3.20.0",
    "react-navigation": "^4.1.1",
    "react-navigation-drawer": "^2.7.2",
    "react-redux": "^8.0.5",
    "redux": "^4.2.1",
    "redux-logger": "^3.0.6",
    "yarn": "^1.22.19",
    "yup": "^1.1.1"
  },
  "devDependencies": {
    "@ant-design/icons-react-native": "^2.3.2",
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native-community/eslint-config": "^3.2.0",
    "@tsconfig/react-native": "^3.0.0",
    "@types/jest": "^29.5.0",
    "@types/react": "^18.0.37",
    "@types/react-test-renderer": "^18.0.0",
    "@types/redux-logger": "^3.0.9",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "babel-jest": "^29.2.1",
    "babel-plugin-import": "^1.13.6",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "metro-react-native-babel-preset": "0.73.9",
    "prettier": "^2.8.8",
    "react-native-vector-icons": "^9.2.0",
    "react-test-renderer": "18.2.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.4"
  }
}
